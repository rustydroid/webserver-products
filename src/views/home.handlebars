<div>
  <h1>Listado Productos</h1>
  <table id="products">
    <caption>Productos</caption>
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Description</th>
        <th>Code</th>
        <th>Price</th>
        <th>Status</th>
        <th>Stock</th>
        <th>Category</th>
        <th>Thumbnails</th>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
        <tr>
          <td>{{this.id}}</td>
          <td>{{this.title}}</td>
          <td>{{this.description}}</td>
          <td>{{this.code}}</td>
          <td>{{this.price}}</td>
          <td>{{this.status}}</td>
          <td>{{this.stock}}</td>
          <td>{{this.category}}</td>
          <td>{{this.thumbnail}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
{{! <script src="/socket.io/socket.io.js"></script> }}
{{! <script src="/js/index.js"></script> }}

